	});
	window.setTimeout(function(){
		MIN.loadJs("http://jqmt.qq.com/cdn_minisite.js",function(){});
		MIN.loadJs("http://mat1.qq.com/js/speed_ns_v10.5-min.js",function(){});
		MIN.loadJs("http://mat1.gtimg.com/www/js/common.js",function(){
			checkNonTxDomain(0.01, 12);
		});
	}, 5000);
}
</script>
